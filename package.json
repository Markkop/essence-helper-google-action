{
  "name": "actions-builder-canvas",
  "version": "0.0.0",
  "private": true,
  "description": "An Action that demonstrates Interactive Canvas.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "eslint --fix \"**/*.js\"",
    "test": "npm run lint",
    "serve": "firebase serve",
    "deploy": "firebase deploy",
    "deploy-project": "firebase deploy --project essence-helper --only hosting",
    "push": "cd sdk && gactions push && cd ..",
    "pull": "cd sdk && gactions pull -f && cd ..",
    "preview": "cd sdk && gactions push && gactions deploy preview && cd .."
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-config-google": "^0.9.1"
  }
}
