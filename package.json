{
  "name": "essence-helper-google-action",
  "version": "0.0.0",
  "private": true,
  "description": "A Google Action that provides information about the Zenith VR MMORPG.",
  "engines": {
    "node": "14.17.2"
  },
  "scripts": {
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "eslint --fix \"**/*.js\"",
    "test": "npm run lint",
    "serve": "firebase serve",
    "deploy": "firebase deploy",
    "deploy-project": "firebase deploy --project essence-helper --only hosting",
    "push": "cd sdk && gactions push && cd ..",
    "pull": "cd sdk && gactions pull -f && cd ..",
    "preview": "cd sdk && gactions deploy preview && cd ..",
    "push-and-preview": "npm run push && npm run preview"
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-config-google": "^0.9.1"
  }
}
